    "dev": "pnpm build:kernel && vite",
    "dev:skip-build": "vite",
    "build": "rm -rf dist && pnpm lint:check && pnpm build:kernel && NODE_OPTIONS=--max-old-space-size=8192 vite build && pnpm postbuild",
    "build:kernel": "pnpm --prefix ../aztec-contracts build && pnpm update @obsidion/kernel",
    "build:backend": "pnpm --prefix ../backend build && pnpm update @obsidion/backend",
    "postbuild": "cp dist/assets/acvm_js_bg-*.wasm public/acvm_js_bg.wasm && cp dist/assets/noirc_abi_wasm_bg-*.wasm public/noirc_abi_wasm_bg.wasm",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
